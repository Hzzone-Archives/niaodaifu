<!--pages/msg_success/msg_success.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">付款成功</view>
            <view class="weui-msg__desc">恭喜你付款成功，请等待你的商品<navigator url="" class="weui-msg__link"></navigator></view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap='see_orders'>查看订单</button>
                <button class="weui-btn" type="default" bindtap='return_home'>返回主页</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link">niaodaifux</navigator>
                </view>
                <view class="weui-footer__text">Copyright © 2008-2016 nidaifu.xyz</view>
            </view>
        </view>
    </view>
</view>